{
  "name": "fir-ui",
  "private": false,
  "version": "0.0.1",
  "description": "A simple set of components for prototypes and small own projects on Vue 3.",
  "main": "src/main.js",
  "files": [
    "src"
  ],
  "keywords": [
    "ui-kit",
    "vue3"
  ],
  "author": "B-Dmitriy <beloyar.dev@outlook.com>",
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@10.8.1",
  "engines": {
    "node": ">=20.8.1"
  },
  "scripts": {
    "build": "vite build",
    "dev": "storybook dev -p 6006",
    "storybook": "storybook -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "semantic-release": "^24.2.0",
    "vue": "3.5.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.2",
    "@storybook/addon-essentials": "8.4.1",
    "@storybook/addon-interactions": "8.4.1",
    "@storybook/addon-onboarding": "8.4.1",
    "@storybook/blocks": "8.4.1",
    "@storybook/test": "8.4.1",
    "@storybook/vue3": "8.4.1",
    "@storybook/vue3-vite": "8.4.1",
    "@vitejs/plugin-vue": "5.1.4",
    "storybook": "8.4.1",
    "vite": "5.4.10"
  },
  "release": {
    "branches": [
      "main"
    ],
    "repositoryUrl": "git@github.com:B-Dmitriy/fir-ui.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package-lock.json",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
